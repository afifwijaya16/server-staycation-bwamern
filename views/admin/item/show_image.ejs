<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="show-tab">
  <div class="card shadow mb-4">
    <div class="card-header">
      <a href="/admin/item" class="btn btn-warning float-right">Back</a>
    </div>

    <div class="row">
      <% for( let x = 0; x < item.imageId.length; x++ ) { %>
      <div class="col-md-4">
        <div class="card-body">
          <img src="https://server-bwa-staycation.herokuapp.com/<%= item.imageId[x].imageUrl %>" alt="imageUrl"
            style="width: 100%; display: block;" />
        </div>
      </div>
      <% } %>
    </div>
  </div>
</div>